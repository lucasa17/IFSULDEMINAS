programa {
  funcao inicio() {

    real lado1, lado2

    escreva("Digite o tamanho(em metros) da frente do terreno: ")
    leia(lado1)

    escreva("Digite o tamanho(em metros) da lateral do terreno: ")
    leia(lado2)

    escreva("\nA área do terreno é de ", lado1 * lado2, " metros quadrados.")
  }
}
